onar.host.url=http://sonarqube.avans-informatica-breda.nl

#
# Add SONAR_LOGIN_KEY and SONAR_PROJECT_KEY environment variables to your
# local system and on your CI/CD system (e.g. GitLab)
#
sonar.login=${env.SONAR_LOGIN_KEY}
sonar.projectKey=${env.SONAR_PROJECT_KEY}
sonar.projectName=${env.SONAR_PROJECT_KEY}
# This should be left blank if an authentication token is being used.
sonar.password=

# Code coverage info is generated by running Angular tests with coverage option.
# ng test --coverage
sonar.typescript.lcov.reportPaths=coverage/${env.SONAR_PROJECT_KEY}/lcov.info

sonar.projectVersion=0.1
sonar.projectDescription=CheckCenter front-end
sonar.links.homepage=https://cc-webapp.herokuapp.com/
sonar.links.ci=https://gitlab.com/check-center/check-center
sonar.links.scm=https://gitlab.com/check-center/check-center
sonar.sourceEncoding=UTF-8
sonar.verbose=false
sonar.working.directory=.scannerwork

# sonar.language=jse
# sonar.log.level=INFO
sonar.projectBaseDir=.
sonar.sources=./src
sonar.sourceEncoding=UTF-8

sonar.exclusions=node_modules/*.css
